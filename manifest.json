{
  "name": "Modus",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Modus",
  "permissions": [ "alarms", "activeTab", "scripting", "tabs", "storage", "cookies"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["background.js","anime.js","bootstrap.js","sweetalert2@11.js","jquery.min.js", "inject.js"],
        "css": ["bootstrap.css","style.css"]
    }
],
  "background": {
    "service_worker": "background.js"
  },
"host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": {
      "32": "logo.png"
    },
    "default_title": "Modus",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Shift+E"
      }
    }
  }
}