{
  "name": "Modus",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Modus :)",

  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["sweetalert2@11.js","jquery.min.js", "inject.js"],
        "css": ["style.css"]
    }
],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"],
"host_permissions": ["<all_urls>"],
  "action": {},
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      }
    }
  }
}